{
  "TranslatorSettings": {
    "Endpoint": "https://api.cognitive.microsofttranslator.com",
    "ApiKey": "Your translator resource API key.",
    "Region": "Your translator resource region."
  },  
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    },
    "Console": {
      "DisableColors": false,
      "LogLevel": {
        "Default": "Information"
      }
    },
    "Debug": {
      "LogLevel": {
        "Default": "Information"
      }
    },
    "Disk": {
      "LogLevel": {
        "Default": "Information"
      },
      "MaxFileSize": 1
    },
    "File": {
      "LogLevel": {
        "Default": "Information"
      },
      "Path": "Logs/app.log",
      "Append": true,
      "FileSizeLimitBytes": 10485760,
      "MaxRollingFiles": 5
    }

  }
}
